void FieldSelect_kyoto_shortcut()
{
    FUNCTION_002C( 14, 3 );
    MSG_SELECT( FLD_ORDER, KYOTO_SHOTCUTSEL, 0 );
    int selection = GET_MSG_SELECTION( 0 );
    
    if ( selection == 0 )
    {
        FADE( 3, 20 );
        FUNCTION_4001( 14, 2, 1, 0 );
        return;
    }
    else if ( selection == 1 )
    {
        FADE( 3, 20 );
        FUNCTION_4001( 14, 3, 2, 0 );
        return;
    }
    else if ( selection == 2 )
    {
        FADE( 3, 20 );
        FUNCTION_4001( 14, 0x10, 2, 0 );
        return;
    }
    else 
    {
        MSG_WND_CLS();
        return;
    }
}