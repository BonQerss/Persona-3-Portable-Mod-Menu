void MMkyoto_shortcut()
{
    FUNCTION_3058();
    int CurMinorField = GET_MSG_SELECTION( 0 );
    int StartPoint2F = 0;
    int StartPoint3F = 0;
    int SelException = 0;
    
    if ( CurMinorField == 2 )
    {
        SelException = 1;
    }
    else if ( CurMinorField == 3 )
    {
        SelException = 2;
    }
    else if ( CurMinorField == 6 )
    {
        SelException = 0;
        StartPoint2F = 2;
    }
    else if ( CurMinorField == 7 )
    {
        SelException = 0;
        StartPoint2F = 3;
    }
    else if ( CurMinorField == 8 )
    {
        SelException = 0;
        StartPoint2F = 4;
    }
    else if ( CurMinorField == 9 )
    {
        SelException = 0;
        StartPoint2F = 5;
    }
    else if ( CurMinorField == 10 )
    {
        SelException = 0;
        StartPoint2F = 6;
    }
    else if ( CurMinorField == 11 )
    {
        SelException = 0;
        StartPoint3F = 2;
    }
    else if ( CurMinorField == 12 )
    {
        SelException = 0;
        StartPoint3F = 3;
    }
    else if ( CurMinorField == 13 )
    {
        SelException = 0;
        StartPoint3F = 4;
    }
    else if ( CurMinorField == 14 )
    {
        SelException = 0;
        StartPoint3F = 5;
    }
    else if ( CurMinorField == 15 )
    {
        SelException = 0;
        StartPoint3F = 6;
    }
    else if ( CurMinorField == 0x10 )
    {
        SelException = 4;
    }
    else 
    {
        SelException = 0;
    }

    FUNCTION_002C( 14, 3 );
    MSG_SELECT( FLD_ORDER, KYOTO_SHOTCUTSEL, SelException );
    int selection = GET_MSG_SELECTION( 0 );
    
    if ( selection == 0 )
    {
        FADE( 3, 20 );
        FUNCTION_4001( 14, 2, 1, 0 );
        return;
    }
    else if ( selection == 1 )
    {
        FADE( 3, 20 );
        FUNCTION_4001( 14, 3, StartPoint2F, 0 );
        return;
    }
    else if ( selection == 2 )
    {
        FADE( 3, 20 );
        FUNCTION_4001( 14, 0x10, StartPoint3F, 0 );
        return;
    }
    else 
    {
        MSG_WND_CLS();
        return;
    }

}

